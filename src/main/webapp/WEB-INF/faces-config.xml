<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://Java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>question2</managed-bean-name>
		<managed-bean-class>modelo.bean.QuestionBean2</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Main2.xhtml</display-name>
		<from-view-id>/Main2.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateQuestion2.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main2.xhtml</display-name>
		<from-view-id>/Main2.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryQuestion2.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
